import java.util.*;

public class Main {
    public static void main(String[] args) {
      System.out.println("======= My Favourite Song  =======");
      System.out.println("======= SIA - Unstoppable  =======");
       printCouplet1();
       printChorus1();
       printChorus2();
          printCouplet2();
          printChorus1();
          printChorus2();
              printChorus1();
              printChorus2();
    }
    
    
    
    public static void printCouplet1() {
       System.out.println("\n======= COUPLET 1 =======");
      System.out.println("All smiles, I know what it takes to fool this town");
      System.out.println("I'll do it 'til the sun goes down and all through the night time");
      System.out.println("Oh yeah");
      System.out.println("Oh yeah, I'll tell you what you wanna hear");
      System.out.println("Leave my sunglasses on while I shed a tear");
      System.out.println("It's never the right time");
      System.out.println("Yeah, yeah....");
    }
    
    public static void printCouplet2() {
      System.out.println("\n======= COUPLET 2 =======");
      System.out.println("Break down, only alone I will cry on out");
      System.out.println("You'll never see what's hiding out");
      System.out.println("Hiding out deep down");
      System.out.println("Yeah, yeah");
      System.out.println("I know, I've heard that to let your feelings go");
      System.out.println("Is the only way to make friendships grow");
      System.out.println("But I'm too afraid nowl");
    }
    
   
    public static void printChorus1() {
      System.out.println("\n======= CHORUS =======");
      System.out.println("Break down, only alone I will cry on out");
      System.out.println("You'll never see what's hiding out");
      System.out.println("Hiding out deep down");
      System.out.println("Yeah, yeah");
      System.out.println("I know, I've heard that to let your feelings go");
      System.out.println("Is the only way to make friendships grow");
      System.out.println("But I'm too afraid nowl");
    }
    
    
     public static void printChorus2() {
      System.out.println("Unstoppable today");
      System.out.println("Unstoppable today");
      System.out.println("Unstoppable today");
}
}
